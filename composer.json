{
    "name": "bfg/entity",
    "description": "Pattern for generating php entities.",
    "type": "bfg-script",
    "keywords": ["bfg", "entity", "script"],
    "homepage": "https://github.com/bfg-s/entity",
    "license": "MIT",
    "authors": [
        {
            "name": "bfg",
            "email": "bfg.script@gmail.com"
        }
    ],
    "prefer-stable": true,
    "require": {
        "php": ">=8.0.0",
        "bfg/installer": "*"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "Bfg\\Entity\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bfg\\Entity\\ServiceProvider"
            ],
            "aliases": {
                "EntityCarrier": "Bfg\\Entity\\Facade"
            }
        }
    }
}
